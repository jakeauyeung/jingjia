{
  "name": "jingjia",
  "version": "1.0.0",
  "description": "a tool for jingjia",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"electron .\"",
    "mac": "electron-packager . 'jingjia' --platform=darwin --arch=x64 --icon=jingjia.icns --out=./dist --overwrite --app-version=1.0.0 --ignore=dev-settings --ignore=node_modules/electron-* --ignore=data.db --ignore=win32.iss --ignore=win64.iss --ignore=jingjia.xls --prune",
    "win32": "electron-packager . jingjia --platform=win32 --arch=ia32 --icon=jingjia.ico --out=./dist --overwrite --app-version=1.0.0 --ignore=dev-settings --ignore=node_modules/electron-* --ignore=data.db --ignore=win32.iss --ignore=win64.iss --ignore=jingjia.xls --prune",
    "win64": "electron-packager . jingjia --platform=win32 --arch=x64 --icon=jingjia.ico --out=./dist --overwrite --app-version=1.0.0 --ignore=dev-settings --ignore=node_modules/electron-* --ignore=data.db --ignore=win32.iss --ignore=win64.iss --ignore=jingjia.xls --prune",
    "linux": "electron-packager . 'jingjia' --platform=linux --arch=x64 --out=./dist --overwrite --app-version=1.0.0 --ignore=dev-settings --ignore=node_modules/electron-* --ignore=data.db --ignore=win32.iss --ignore=win64.iss --ignore=jingjia.xls --prune",
    "clean": "rm -rf dist",
    "build": "npm run mac && npm run win32 && npm run win64 && npm run linux",
    "win": "npm run win32 && npm run win64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakeauyeung/jingjia.git"
  },
  "author": "jakeauyeung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jakeauyeung/jingjia/issues"
  },
  "homepage": "https://github.com/jakeauyeung/jingjia#readme",
  "devDependencies": {
    "concurrently": "^3.1.0",
    "electron": "^1.4.8",
    "electron-packager": "^8.3.0"
  },
  "dependencies": {
    "nedb": "^1.8.0",
    "node-xlsx": "^0.7.4"
  }
}
